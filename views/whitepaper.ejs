    <script>
        document.addEventListener("DOMContentLoaded", () => {
            // Function to scroll to the page specified in the URL fragment
            function scrollToPageFromHash() {
                const hash = window.location.hash; // e.g., "#page=3"
                if (hash.startsWith('#page=')) {
                    const pageNumber = hash.split('=')[1];
                    const target = document.getElementById(`page-${pageNumber}`);
                    if (target) {
                        target.scrollIntoView({ behavior: 'smooth' });
                    }
                }
            }

            // Wait for all images to load before attempting to scroll
            function waitForImagesToLoad() {
                const images = document.querySelectorAll('.page img');
                let loadedImages = 0;
                const totalImages = images.length;

                // Function to check if all images are loaded
                function checkImagesLoaded() {
                    loadedImages++;
                    if (loadedImages === totalImages) {
                        // Once all images are loaded, scroll to the page
                        scrollToPageFromHash();
                    }
                }

                // Attach load event to each image
                images.forEach(img => {
                    if (img.complete) {
                        checkImagesLoaded(); // If already loaded, trigger immediately
                    } else {
                        img.addEventListener('load', checkImagesLoaded);
                    }
                });
            }

            // Scroll on page load if a fragment exists
            waitForImagesToLoad();

            // Listen for hash changes (e.g., navigating back/forward in browser)
            window.addEventListener('hashchange', () => {
                waitForImagesToLoad(); // Recheck and scroll on hash change
            });

            // Expandable content toggle
            const showMoreBtn = document.getElementById("show-more-btn");
            const hiddenContent = document.getElementById("hidden-content");

            showMoreBtn.addEventListener("click", () => {
                if (hiddenContent.style.display === "none" || hiddenContent.style.display === "") {
                    hiddenContent.style.display = "block";
                    showMoreBtn.textContent = "Show Less";
                } else {
                    hiddenContent.style.display = "none";
                    showMoreBtn.textContent = "Show the Whitepaper as a Text";
                }
            });
        });
    </script>
